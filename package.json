{
  "name": "pomotimer",
  "displayName": "PomoTimer",
  "description": "A fancy pomodoro timer",
  "version": "0.1.0",
  "publisher": "afractal",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pomotimer.displayTimer"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "title": "Start Pomodoro Timer",
        "command": "pomotimer.displayTimer"
      }
    ],
    "keybindings": [
      {
        "command": "pomotimer.displayTimer",
        "key": "shift+p",
        "win": "shift+p"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^6.0.40",
    "mocha": "^3.2.0",
    "typemoq": "^1.4.1",
    "typescript": "^2.2.1",
    "vscode": "^1.1.0"
  }
}
